{
  "dashboard": {
    "id": null,
    "title": "DevOps Production Monitoring Dashboard",
    "tags": ["devops", "production", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "ðŸš€ System Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "refId": "A",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"displayMode": "list", "orientation": "horizontal"},
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "UP", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1}
      },
      {
        "id": 3,
        "title": "Total HTTP Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(prometheus_http_requests_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1}
      },
      {
        "id": 4,
        "title": "Memory Usage (MB)",
        "type": "stat",
        "targets": [
          {
            "expr": "go_memstats_alloc_bytes / 1024 / 1024",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "decbytes",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1}
      },
      {
        "id": 5,
        "title": "Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "time() - process_start_time_seconds",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 300},
                {"color": "green", "value": 3600}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1}
      },
      {
        "id": 6,
        "title": "Request Rate (req/sec)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(prometheus_http_requests_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "reqps",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 5}
      },
      {
        "id": 7,
        "title": "Response Time Distribution",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(prometheus_http_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 5}
      },
      {
        "id": 8,
        "title": "Active Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "prometheus_notifications_queue_length",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 5}
      },
      {
        "id": 9,
        "title": "ðŸ“Š Performance Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
        "collapsed": false
      },
      {
        "id": 10,
        "title": "HTTP Request Rate Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(prometheus_http_requests_total[5m])",
            "refId": "A",
            "legendFormat": "{{method}} {{handler}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "gradientMode": "opacity"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10}
      },
      {
        "id": 11,
        "title": "Memory Usage Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "go_memstats_alloc_bytes",
            "refId": "A",
            "legendFormat": "Allocated Memory"
          },
          {
            "expr": "go_memstats_heap_inuse_bytes",
            "refId": "B",
            "legendFormat": "Heap In Use"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "gradientMode": "opacity"
            },
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10}
      },
      {
        "id": 12,
        "title": "HTTP Status Code Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (code) (prometheus_http_requests_total)",
            "refId": "A",
            "legendFormat": "{{code}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 18}
      },
      {
        "id": 13,
        "title": "Goroutines Count",
        "type": "timeseries",
        "targets": [
          {
            "expr": "go_goroutines",
            "refId": "A",
            "legendFormat": "Goroutines"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 18}
      },
      {
        "id": 14,
        "title": "GC Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(go_gc_duration_seconds_sum[5m])",
            "refId": "A",
            "legendFormat": "GC Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 18}
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "links": []
  },
  "overwrite": true
}